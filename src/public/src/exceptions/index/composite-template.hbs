<div class="page-header">
    <h1>Exceptions:</h1>
</div>

<div class="list-group"></div>

<div class="text-center">
    <ul class="pagination">
        {{#if prev}}
            <li><a href="#exceptions?page={{prev}}">&laquo;</a></li>
        {{else}}
            <li class="disabled"><a>&laquo;</a></li>
        {{/if}}

        {{#each pages}}
            {{#if current}}
                <li class="active"><a>{{page}} <span class="sr-only">(current)</span></a></li>
            {{else}}
                <li><a href="#exceptions?page={{page}}">{{page}}</a></li>
            {{/if}}
        {{/each}}

        {{#if next}}
            <li><a href="#exceptions?page={{next}}">&raquo;</a></li>
        {{else}}
            <li class="disabled"><a>&raquo;</a></li>
        {{/if}}
    </ul>
</div>
